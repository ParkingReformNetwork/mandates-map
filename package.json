{
  "name": "mandates-map",
  "version": "0.1.0",
  "source": "index.html",
  "type": "module",
  "scripts": {
    "start": "parcel",
    "build": "rm -rf dist; parcel build --detailed-report",
    "test": "playwright test",
    "check": "tsc --noEmit",
    "fmt": "prettier --write .",
    "fix": "prettier --write scripts/ tests/; eslint --fix scripts/  tests/",
    "lint": "prettier --check . && eslint scripts/ tests/",
    "update-city-detail": "node scripts/updateCityDetail.js",
    "update-map-data": "node scripts/updateMapData.js",
    "update-lat-lng": "node scripts/updateLatLng.js",
    "broken-links": "node scripts/brokenLinks.js",
    "serve-dist": "cd dist; http-server",
    "test-dist": "PORT=8080 playwright test"
  },
  "devDependencies": {
    "@parcel/compressor-brotli": "^2.9.3",
    "@parcel/compressor-gzip": "^2.9.3",
    "@parcel/transformer-sass": "^2.9.3",
    "@playwright/test": "^1.35.1",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.37.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "handlebars": "^4.7.7",
    "http-server": "^14.1.1",
    "jsdom": "^22.1.0",
    "luxon": "^3.3.0",
    "node-fetch": "^2.6.11",
    "node-geocoder": "^4.2.0",
    "papaparse": "^5.4.1",
    "parcel": "^2.9.2",
    "parcel-transformer-csv": "^0.0.3",
    "playwright": "^1.35.1",
    "prettier": "^2.8.7",
    "typescript": "^5.2.2"
  },
  "targets": {
    "default": {
      "publicUrl": "./"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@types/leaflet": "^1.9.4",
    "choices.js": "^10.2.0",
    "leaflet": "^1.9.4"
  }
}
