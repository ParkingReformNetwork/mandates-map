{
  "name": "mandates-map",
  "version": "0.1.0",
  "source": "index.html",
  "type": "module",
  "scripts": {
    "start": "parcel",
    "build": "rm -rf dist; parcel build --detailed-report",
    "test": "playwright test",
    "check": "tsc --noEmit",
    "fmt": "prettier --write .",
    "fix": "prettier --write .; eslint --fix scripts/  tests/",
    "lint": "prettier --check . && eslint scripts/ tests/",
    "update-city-detail": "tsx scripts/updateCityDetail.ts",
    "update-map-data": "tsx scripts/updateMapData.ts",
    "sync-lat-lng": "tsx scripts/syncLatLng.ts",
    "broken-links": "tsx scripts/brokenLinks.ts",
    "serve-dist": "cd dist; http-server",
    "test-dist": "PORT=8080 playwright test"
  },
  "devDependencies": {
    "@parcel/compressor-brotli": "~2.11.0",
    "@parcel/compressor-gzip": "~2.11.0",
    "@parcel/transformer-sass": "~2.11.0",
    "@playwright/test": "^1.35.1",
    "@types/jsdom": "^21.1.7",
    "@types/leaflet": "^1.9.4",
    "@types/lodash-es": "^4.17.12",
    "@types/luxon": "^3.4.2",
    "@types/node-fetch": "^2.6.11",
    "@types/node-geocoder": "^4.2.6",
    "@types/papaparse": "^5.3.14",
    "@types/tabulator-tables": "^6.2.3",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "handlebars": "^4.7.7",
    "http-server": "^14.1.1",
    "jsdom": "^24.1.1",
    "node-fetch": "^2.6.11",
    "node-geocoder": "^4.2.0",
    "papaparse": "^5.4.1",
    "parcel": "~2.11.0",
    "parcel-transformer-csv": "^0.0.3",
    "playwright": "^1.35.1",
    "prettier": "^3.3.3",
    "tsx": "^4.16.5",
    "typescript": "^5.2.2"
  },
  "targets": {
    "default": {
      "publicUrl": "./"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "choices.js": "^11.0.1",
    "leaflet": "^1.9.4",
    "lodash-es": "^4.17.21",
    "luxon": "^3.3.0",
    "tabulator-tables": "^6.2.5"
